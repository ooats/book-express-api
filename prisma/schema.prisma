generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model books {
  id        Int     @id @default(autoincrement())
  isbn      String @unique @db.VarChar(50)
  booktitle String @db.VarChar(50)
  author    String @db.VarChar(50)
}
